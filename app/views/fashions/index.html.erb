<div class="container">
<div class="row mt-5">
 <div class="col-md-4.col-12">
 <%= link_to following_fashions_user_path(@fashions) do %>
 <button type="button" class="btn btn-info mb-3">Follower</button>
 <% end %>
 <%= render 'search_form', url: fashions_path, q: @q %>
  <% if @fashions.any? %>
  <%= render @fashions %>
  <% else %>
   <h3><%= '検索結果がありません。' %></h3>
  <% end %>
  <%= paginate @fashions %>
 </div>
 <div class="col-md-4.col-12 mt-5">
  <div class="ranks-box ml-5">
   <div class="card ml-5 mt-5">
     <h3>人気の投稿</h3>
   </div>
   <div class="card-body ml-5">
    <%= render 'rank', all_ranks: @all_ranks, a: @a %>
   </div>
  </div>
 </div>
</div>
</div>

